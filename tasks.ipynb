   ],
   "source": [
    "def rotl(x, n=1):\n",
    "    # Trying to rotate bits to the left\n",
    "    return (x << n) | (x >> (32 - n))\n",
    "\n",
    "# Test\n",
    "print(bin(rotl(0b0001, 1)))\n"
   ]
  }
